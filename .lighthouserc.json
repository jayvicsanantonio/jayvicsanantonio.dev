{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000/",
        "http://localhost:3000/projects",
        "http://localhost:3000/work"
      ],
      "startServerCommand": "pnpm start",
      "startServerReadyPattern": "started server",
      "startServerReadyTimeout": 120000,
      "numberOfRuns": 1,
      "settings": {
        "preset": "desktop"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
